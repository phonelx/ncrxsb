	}else{
		key = $("input_search").val();
	}
	var html="";
	$.ajax( {
		type : 'post',
		url : 'queryChileStents.do',
		dataType : 'json',
		async : false,
		data : {
			squ:obj,
			rows:pageRowNum,
			page:pager,
			key:key
		},
		success : function(data) {
			
			records=data.total;
			currentPage=data.pageNumber;
			total = data.pageCnt;
			pageRowNum=data.pageSize;
