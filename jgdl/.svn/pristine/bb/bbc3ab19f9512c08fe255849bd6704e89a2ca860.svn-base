.img_list {
    display: block;
    padding1: 4px;
    line-height: 1;
    border: 1px solid #ddd;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075);
    -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075);
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075);
     z-index: 2;
    position: relative;
    margin-bottom: 15px !important;
    cursor:pointer;
}
.img_list:hover {
	margin-top1:5px;
	-webkit-box-shadow: 0 3px 6px rgba(0, 0, 0, 0.095);
    -moz-box-shadow: 0 3px 6px rgba(0, 0, 0, 0.095);
    box-shadow: 0 3px 6px rgba(0, 0, 0, 0.095);
}
.img_list a>img{
	width:100%;
	margin-right: auto;
  	margin-left: auto;
  	display: block;
    max-width: 100%;
    height:100%;
    vertical-align: middle;
    border-radius:4px 4px 0 0;
  	}
.img_list img, .img_list > a {
    z-index: 2;
   /*  height: 100px;
    width: 100px; */
    position: relative;
    display: block;
}
.img_list .caption {
    padding: 9px;
}
