@charset "utf-8";
/* CSS Document */
body{ padding:0; margin:0;}
ul,li{ list-style:none;}
a,img{ border:none;}
div{ over-flow:hidden;}

#Top{ width:100%; height:100px; background-image:url(../images/TopBg.png);}
#Top .TopImg{ width:100%; background-image:url(../images/region/Top02.png); background-repeat:no-repeat; background-position:left; height:100px;}
#Top .TopImg ul{ float:right; width:344px; height:60px; margin-top:40px; margin-bottom:0;vertical-align:bottom; background-image:url(../images/BtBg.png)}
#Top .TopImg ul li{ float:right; margin-left:8px; height:60px; overflow:hidden;} 
