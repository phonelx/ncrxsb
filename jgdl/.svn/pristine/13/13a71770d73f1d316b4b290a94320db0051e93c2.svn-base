<struts>

	<package name="goodsHouse" extends="monitor-struts-default">
		
		<!-- 跳转到应用管理页面 -->
		 <action name="goStentsIndex" class="stentsAction" method="goStentsIndex">
			<result name="success">pages/goodsHouse/stentsTree.jsp</result>
		</action> 
		<!--  查询部件-->
		<action name="queryChileStents" class="stentsAction" method="queryChileStents">
