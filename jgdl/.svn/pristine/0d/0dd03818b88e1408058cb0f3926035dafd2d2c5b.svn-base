/**请求的css**/
a{text-decoration: none; font-weight: normal;color: blue; }
	body{ padding: 0px; margin: 0px; font-size: 12px; }
	#main{}
	#main .context{text-align: center;padding: 3px;}
	#contextName{font-weight1: bold; border: 1px solid silver;text-align: center;  width: 300px;height: 24px ; }
	#formDiv{width: 860px;margin: 10px auto; }
	
	#tabs{ overflow: auto; }
	#tabDiv { position: absolute; z-index: 0;top: 150px;left: 0px; }
	#tabs .tabPage{text-align: center;width: 1160px; background: white;display: none;}
	#tabs .tabPage td{border: 1px solid rgb(222, 222, 222); width: 130px;color: black;cursor: pointer; }
	#tabs .tabPage td:HOVER{background-color: rgb(2, 123, 228); color: white; }
	#tabs .tabPage .checked{background-color: rgb(2, 123, 228); color: white; }
	#ulPage ,#ulPage li{list-style-type: none; padding: 0px;margin: 0px;}
	#ulPage li{border: 1px solid rgb(222, 222, 222); width: 130px;color: black;cursor: pointer; padding: 7px 0px;margin-top: 5px; }
	#ulPage li:HOVER{background-color: rgb(2, 123, 228); color: white; }
	#ulPage .checked{background-color: rgb(2, 123, 228); color: white; }
	#ulCmt li,#ulCmt{list-style-type: none; padding: 0px;margin: 0px;}
	#ulCmt li{border: 1px solid rgb(222, 222, 222); width: 130px;color: black;cursor: pointer; padding: 7px 0px;margin-top: 5px; }
	#tabs #contentDiv{ width: 1158px;margin-left: 160px;;}
	
	.type_tab td{text-align: left;}