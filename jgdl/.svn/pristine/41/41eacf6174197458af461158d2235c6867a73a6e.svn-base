package monitor.manage.bean.dto;

import monitor.manage.bean.entity.DBSourceEntity;

/**
 * @ClassName:SourceDto
 * @dateTime: 2016-3-17 下午1:11:24
 * @version
 * @author: 张家俊
 * @since JDK 1.6 History： Editor version Time Operation
 */
public class SourceDto {
	private int squ;
	private String title;
	private String checkedBox;

	public SourceDto() {

	}

	public SourceDto(DBSourceEntity dbSource) {
		this.setSqu(dbSource.getSqu());
		this.setTitle(dbSource.getTitle());
	}

	public int getSqu() {
		return squ;
	}

	public void setSqu(int squ) {
		this.squ = squ;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getCheckedBox() {
		return checkedBox;
	}

	public void setCheckedBox(String checkedBox) {
		this.checkedBox = checkedBox;
	}
}
