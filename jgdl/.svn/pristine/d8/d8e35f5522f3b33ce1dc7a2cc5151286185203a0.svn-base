}
.select-res p {
	position:relative;
	width1: 200px;
	height:40px;
	line-height:40px;
	border:dashed 1px #2071a1;
	padding:0 5px;
	text-overflow:ellipsis;
	white-space:nowrap;
	overflow:hidden;
	padding-right:20px;
	border-radius:4px;
	margin-top:10px;
	background-color: #e5eef9;
}
.select-res a {
	position:absolute;
	top:1px;
	right:0;
	display: block;
	width:28px;
	height:28px;
	/* background:url(../../images/level/close_btn.jpg) no-repeat center; */
	cursor:pointer;
}
.select-res input {
	width:172px;
	height:14px;
	line-height:14px;
	margin-left:4px;
	padding:7px 5px;
	border:solid 1px #c0c0c0;
	color:#2c2c2c;
}
.select-res span.selected {
	color:#626262;
}
.select-box {
	border:solid 1px #ec5c00;
	width:714px;
	height:220px;
	padding:20px 20px 10px;
	position:absolute;
	display:none;
	z-index:5;
	background: #fff;
	text-align:left;
	margin-left:122px;
}
.select-box .close-btn {
	position:absolute;
	right:5px;
	top:5px;
	cursor: pointer;
}
.select-box .wrap {
	height:200px;
	border:solid 1px #ddd;
	margin-top:5px;
}
.select-box ul {
	float:left;
	width:178px;
	height:200px;
	overflow-y: scroll;
}
.select-box ul li {
	height1:25px;
	line-height:22px;
	padding:0 10px;
	background: url(../../images/level/arr.gif) no-repeat 152px center;
	color:#626262;
	cursor: pointer;
}
.select-box ul li.selected {
	background:url(../../images/level/arr_selected.gif) no-repeat 152px center #ec5c00;
	color:#fff;
}