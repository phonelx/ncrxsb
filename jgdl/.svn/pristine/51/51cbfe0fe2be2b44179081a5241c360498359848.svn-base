function parentload(obj){
	var ssmc = $(obj).attr("ssmc")

	var squ = $(obj).attr("id");
	var zbm = $(obj).attr("title");

	$("#jdlx").html(zbm);
	$("#fla").val( $(obj).attr("flag"))
	
	 if(ssmc=="产品系列"){
			
			$(".divider1").html(ssmc);
			$(".divider2").html("");
			$(".divider3").html("");
			$(".divider4").html("");
			
			
	}else if(ssmc=="CPXL"){
	
		$(".divider2").html("/"+zbm);
		$(".divider3").html("");
		$(".divider4").html("");
		
		
	}else if(ssmc=="CPZM"){
	
		$(".divider3").html("/"+zbm);
		$(".divider4").html("");
	
	}else if(ssmc=="CPTZ"){
		
	
		$(".divider4").html("/"+zbm);
	}
	$("#navg").text($(obj).attr("title"));
	$("#pd").val($(obj).attr("id"));
	$("#fid").val($(obj).attr("id"));	
	loadMenu(obj);
	searchWord();
}

