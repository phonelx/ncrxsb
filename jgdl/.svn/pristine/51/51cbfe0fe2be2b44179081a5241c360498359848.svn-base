var query="";
function searchWord(){
	var word = $("#input_search").val();
	
	if (word=="关键字搜索") {
		query ="1";
	}else{
		query=$("#input_search").val();
	} 
	
	var fla = $("#fla").val();
	if(fla == 1){
		gridload($("#pd").val());
	}else if(fla == 3){
		gridloadGd()
	}else if(fla == 4){
		gridloadAz()
	}else if(fla == 5){
		//gridloadAz()
	}else if(fla == 9){
		gridload("-1")
	}
	
}  

