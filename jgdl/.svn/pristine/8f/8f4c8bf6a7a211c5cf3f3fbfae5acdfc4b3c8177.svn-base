package monitor.common.bean.vo;

/** 
 * Ztree 树形结构 节点 实体bean
 * @author  liangxj
 * @datetime  Apr 27, 2011 1:50:49 PM
 */
public class ZTreeNodeVo {
	private String id ;
	private String name ;
	private String isParent ;
	private String optFlag="" ;//操作标识
	/** 
	 * @return id 
	 */
	public String getId() {
		return id;
	}
	/** 
	 */
	
	public void setId(String id) {
		this.id = id;
	}
	/** 
	 * @return name 
	 */
	public String getName() {
		return name;
	}
	/** 
	 */
	
	public void setName(String name) {
		this.name = name;
	}
	/** 
	 * @return isParent 
	 */
	public String getIsParent() {
		return isParent;
	}
	/** 
	 */
	
	public void setIsParent(String isParent) {
		this.isParent = isParent;
	}
    /**
     * @return the optFlag
     */
    public String getOptFlag() {
        return optFlag;
    }
    /**
     * @param optFlag the optFlag to set
     */
    public void setOptFlag(String optFlag) {
        this.optFlag = optFlag;
    }
	
}
