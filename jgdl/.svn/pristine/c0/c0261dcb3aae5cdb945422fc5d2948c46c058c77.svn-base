			pageDiv(records,total,currentPage);
		},
		error : function() {
			Modal.alert(
			  {
			    msg: '应用加载出错',
			    title: '消息提示',
			    btnok: '确定',
			    btncl:'取消'
			    	
			  });
		}
	});
}

