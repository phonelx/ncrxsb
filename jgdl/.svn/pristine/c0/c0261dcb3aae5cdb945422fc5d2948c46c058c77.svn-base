//加载表格
var grid ;
var pageNo =1;
function gridload(pager){
	var key = $("input_search").val();
	if(key=="关键字搜索"){
		key = "1";
	}else{
		key = $("input_search").val();
	}
	var html="";
	$.ajax( {
		type : 'post',
		url : 'queryZhbjInfo.do',
		dataType : 'json',
		async : false,
		data : {
			rows:pageRowNum,
			page:pager,
			key:key,
		},
		success : function(data) {
			
			records=data.total;
			currentPage=data.pageNumber;
			total = data.pageCnt;
			pageRowNum=data.pageSize;
			if(data.rows.length==0){
				$("#box_body").html("");
				top=1;
			}
			html+=" <tr>";
			html+=" <th>图片</th>";
			html+=" <th>组合部件名称</th>";
			html+=" <th>管道类型</th>";
			html+=" <th>支架形式</th>";
			html+=" <th>安装方式</th>";
			html+=" <th colspan='2'>操作</th>";
			html+=" </tr>";
			list1 = data.rows
		for(var i = 0;i<list1.length;i++){
			
			html+="<tr>";     
			html+=" <td><img class='dashboard-avatar' src='/upload/"+list1[i].ZP+"'></td>";
			html+=" <td>"+list1[i].DXZHMC+"</td>";
			html+=" <td>"+list1[i].GDLX+"</td>";
			html+=" <td>"+list1[i].ZJXS+"</td>";
			html+=" <td>"+list1[i].AZFS+"</td>";
			//html+=" <td><a onclick='' title='查看' href='javascript:void(0);'> <i class='icon-zoom-in icon-black'></i></a>&nbsp";
			html+=" <td><a onclick='editZhbj(\""+list1[i].ZHSQU+"\")' title='编辑' href='javascript:void(0);'><i class='icon-edit icon-black'></i></a>&nbsp";
			html+=" <a onclick='deleteZhbj(\""+list1[i].ZHSQU+"\")' title='删除' href='javascript:void(0)'><i class='icon-trash icon-black'></i></a></td>";			

		}
			html+="</tr>";     
			$("#box_body").html(html);
			pageDiv(records,total,currentPage);
		},
		error : function() {
			Modal.alert(
			  {
			    msg: '应用加载出错',
			    title: '消息提示',
			    btnok: '确定',
			    btncl:'取消'
			    	
			  });
		}
	});
}
